# Required (default configured for OpenAI)
OPENAI_API_KEY=sk-***

# Optional: Use a different model provider
# ANTHROPIC_API_KEY=sk-ant-***
# GOOGLE_API_KEY=***

# Exa API key for web research
# EXA_API_KEY=***

# Database (defaults work out of the box)
# DB_USER=ai
# DB_PASS=ai
# DB_DATABASE=ai

# Optional engine mode: agno (default) or native
# DASH_ENGINE=native

# Optional native runtime DB URL override
# DASH_NATIVE_DB_URL=sqlite+pysqlite:///./native_dash.db

# Optional personal runtime DB URL override
# DASH_PERSONAL_DB_URL=sqlite+pysqlite:///./personal_dash.db
# DASH_PERSONAL_VECTOR_DIM=256

# Gmail connector (OAuth token exchange already completed)
# GMAIL_ACCESS_TOKEN=
# OR:
# GMAIL_CLIENT_ID=
# GMAIL_CLIENT_SECRET=
# GMAIL_REFRESH_TOKEN=
# GMAIL_SYNC_QUERY=newer_than:365d

# Slack connector
# SLACK_USER_TOKEN=
# SLACK_CONVERSATIONS=C12345,D12345

# iMessage connector
# IMESSAGE_DB_PATH=~/Library/Messages/chat.db

# Native SQL guardrails
# DASH_SQL_DEFAULT_LIMIT=50
# DASH_SQL_MAX_LIMIT=500
# DASH_SQL_TIMEOUT_MS=15000
# DASH_MAX_SQL_ATTEMPTS=3
